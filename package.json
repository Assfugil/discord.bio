{
	"name": "discord.bio",
	"version": "7.0.0-dev",
	"description": "a node.js wrapper + cli client of the discord.bio api",
	"main": "dist/index.js",
	"types": "types/index.d.ts",
	"scripts": {
		"test": "node test.js",
		"build": "tsc",
		"prepare": "npm run build && npm run-script docgen",
		"docgen": "typedoc --mode modules --out docs --theme themes src/index.ts",
		"build-bin": "pkg . --targets node12.16.1-linux-x64,node12.16.1-macos-x64,node12.16.1-win-x64,node12.16.1-alpine-x64,node12.2.0-linux-armv7 --out-path bin"
	},
	"author": "asdfugil",
	"license": "BSD-3-Clause",
	"dependencies": {
		"@types/node-fetch": "^2.5.4",
		"@types/ws": "^7.2.4",
		"colors": "^1.4.0",
		"commander": "^5.1.0",
		"discord.js": "12.2.0",
		"form-data": "^3.0.0",
		"node-fetch": "^2.6.0",
		"@types/node": "^13.13.10"
	},
	"keywords": [
		"discord.bio"
	],
	"devDependencies": {
		"command-exists": "^1.2.9",
		"pkg": "^4.4.8",
		"typedoc": "^0.17.7",
		"typescript": "^3.7.5"
	},
	"bin": {
		"dbio": "dist/cli/cli.js"
	},
	"repository": "github:Assfugil/discord.bio"
}

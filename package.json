{
	"name": "discord.bio",
	"version": "8.2.2",
	"description": "a node.js wrapper + cli client of the discord.bio api",
	"main": "dist/index.js",
	"types": "types/index.d.ts",
	"scripts": {
		"test": "node test.js",
		"build": "tsc",
		"prepare": "npm run build && npm run-script docgen",
		"docgen": "typedoc --mode modules --out docs --theme themes --readme WELCOME.md src/index.ts && touch docs/.nojekyll && echo 'discord-bio.js.org' > docs/CNAME",
		"build-bin": "npm run build && pkg . --targets node12.16.1-linux-x64,node12.16.1-macos-x64,node12.16.1-win-x64,node12.16.1-alpine-x64,node12.2.0-linux-armv7 --out-path bin"
	},
	"author": "asdfugil",
	"license": "BSD-3-Clause",
	"dependencies": {
		"@types/websocket": "^1.0.1",
		"@types/ws": "^7.2.4",
		"colors": "^1.4.0",
		"deepmerge": "^4.2.2",
		"discord.js": "12.2.0",
		"form-data": "^3.0.0",
		"node-fetch": "^2.6.0",
		"websocket": "^1.0.31"
	},
	"keywords": [
		"discord.bio"
	],
	"devDependencies": {
		"@types/node": "^13.13.14",
		"@types/node-fetch": "^2.5.7",
		"pkg": "^4.4.8",
		"typedoc": "^0.17.7",
		"typescript": "^3.7.5"
	},
	"repository": "github:Assfugil/discord.bio",
	"homepage": "https://discord-bio.js.org"
}
